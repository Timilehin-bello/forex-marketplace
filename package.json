{
  "name": "@forex-marketplace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "// Build Scripts": "Scripts for building services and libraries",
    "build:libs": "nx run-many --target=build --projects=shared-types,shared-utils,database,auth,grpc,message-queue",
    "build:all": "nx run-many --target=build --all",
    "build:user-auth": "nx build user-auth-service",
    "build:wallet": "nx build wallet-service",
    "build:rate": "nx build rate-service",
    "build:transaction": "nx build transaction-service",
    "build:notification": "nx build notification-service",
    "// Development Scripts": "Scripts for running services in development mode",
    "dev:user-auth": "nx serve user-auth-service",
    "dev:wallet": "nx serve wallet-service",
    "dev:rate": "nx serve rate-service",
    "dev:transaction": "nx serve transaction-service",
    "dev:notification": "nx serve notification-service",
    "dev:all": "nx run-many --target=serve --all --parallel --maxParallel=5",
    "// Test Scripts": "Scripts for running tests",
    "test:all": "nx run-many --target=test --all",
    "test:user-auth": "nx test user-auth-service",
    "test:wallet": "nx test wallet-service",
    "test:rate": "nx test rate-service",
    "test:transaction": "nx test transaction-service",
    "test:notification": "nx test notification-service",
    "test:libs": "nx run-many --target=test --projects=shared-types,shared-utils,database,auth,grpc,message-queue",
    "test:coverage": "nx run-many --target=test --all --codeCoverage",
    "// Lint Scripts": "Scripts for linting code",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "// Docker Scripts": "Scripts for Docker operations",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:up:detached": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "// Migration Scripts": "Scripts for database migrations",
    "migration:generate": "nx run database:migration:generate --name=",
    "migration:run": "nx run database:migration:run",
    "migration:revert": "nx run database:migration:revert",
    "// Combined Scripts": "Combined scripts for common operations",
    "prepare:dev": "yarn build:libs && yarn docker:up",
    "reset:db": "docker-compose down -v && docker-compose up -d postgres",
    "clean": "nx reset && rm -rf node_modules && yarn cache clean",
    "reinstall": "yarn clean && yarn install",
    "start:dev": "yarn prepare:dev && yarn dev:all",
    "start:prod": "yarn build:all && node dist/apps/user-auth-service/main.js",
    "// CI/CD Scripts": "Scripts for continuous integration",
    "ci": "yarn lint && yarn test:all && yarn build:all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:graph": "nx affected:graph"
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.13.0",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "20.6.1",
    "@nx/eslint-plugin": "20.6.1",
    "@nx/jest": "20.6.1",
    "@nx/js": "20.6.1",
    "@nx/nest": "^20.6.1",
    "@nx/node": "20.6.1",
    "@nx/web": "20.6.1",
    "@nx/webpack": "20.6.1",
    "@nx/workspace": "20.6.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "20.6.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^5.1.4"
  }
}
